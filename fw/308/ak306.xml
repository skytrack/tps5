   </option>
   <option>
    <title>Аналоговый датчик</title>
    <id>11</id>
    <actions>
     <hide>adc1 threshold</hide>
    </actions>      
   </option>
   <option>
    <title>Зажигание</title>
    <id>1</id>
   </option>
   <option>
    <title>Сигнал запуска двигателя</title>
    <id>2</id>
   </option>
   <option>
    <title>Дискретный датчик</title>
    <id>3</id>
   </option>
   <option>
    <title>Тревожная кнопка</title>
    <id>10</id>
